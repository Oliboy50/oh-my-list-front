{
  "name": "anime-list-front",
  "version": "1.0.0",
  "description": "anime-list-front",
  "author": "Oliboy50",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@mdi/font": "2.2.43",
    "@nuxtjs/axios": "5.1.1",
    "@oliboy50/listal-exporter": "0.1.1",
    "cookie": "0.3.1",
    "js-cookie": "2.2.0",
    "jwt-decode": "^2.2.0",
    "nuxt": "1.4.0",
    "nuxt-buefy": "0.0.4"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.15",
    "ajv": "^6.4.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "vue-jest": "^2.5.0",
    "vue-style-loader": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "clearMocks": true
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          ["env", { "targets": { "node": "current" }}]
        ]
      }
    }
  }
}
